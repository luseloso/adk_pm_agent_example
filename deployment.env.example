# Agent Engine Deployment Configuration
# Copy this file to deployment.env and fill in your values

# Tracing configuration (new standard as of January 2026)
# Enables agent traces and logs in Cloud Trace
GOOGLE_CLOUD_AGENT_ENGINE_ENABLE_TELEMETRY=true

# Enables logging of input prompts and output responses in traces
# Set to false if you don't want prompts/responses in traces
OTEL_INSTRUMENTATION_GENAI_CAPTURE_MESSAGE_CONTENT=true

# MCP Server URL (from prd_mcp_server deployment)
# Example: https://prd-mcp-server-xxxxx-uc.a.run.app
MCP_SERVER_URL=https://your-mcp-server.run.app

# GCP Configuration (optional if using --project and --region CLI flags)
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
